{
  "name": "ai-chatbox",
  "version": "0.1.0",
  "description": "A community built AI chat component used by various OSS Ventures startups",
  "type": "module",
  "main": "dist/ai-chatbox.js",
  "module": "dist/ai-chatbox.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ai-chatbox.js",
      "types": "./dist/types/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "component",
    "chat",
    "ai",
    "chatbox"
  ],
  "author": "OSS Ventures",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OSS-Ventures/ai-chatbox.git"
  },
  "bugs": {
    "url": "https://github.com/OSS-Ventures/ai-chatbox/issues"
  },
  "homepage": "https://github.com/OSS-Ventures/ai-chatbox#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.0"
  },
  "packageManager": "pnpm@8.10.0+sha1.77d568bacf41eeefd6695a7087c1282433955b5c"
}
